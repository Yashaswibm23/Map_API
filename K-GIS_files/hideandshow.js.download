$("#attrQuery").click(function () {
    if (heatmapLayer != undefined) {
        map.removeLayer(heatmapLayer);
    }

    if (clusterLayer != undefined) {
        map.removeLayer(clusterLayer);
    }
    for (var i = 0; i < removableLayersArr.length; i++) {
        map.removeLayer(removableLayersArr[i]);
    }
    $("#queries").show();
    $("#buffer").hide();
    $("#Cmplxbuffer").hide();
    $("#Theme_transp").hide();
});



$("#NearBy").click(function () {
    if (heatmapLayer != undefined) {
        map.removeLayer(heatmapLayer);
    }

    if (clusterLayer != undefined) {
        map.removeLayer(clusterLayer);
    }
    for (var i = 0; i < removableLayersArr.length; i++) {
        map.removeLayer(removableLayersArr[i]);
    }
    $("#Cmplxbuffer").hide();
    $("#queries").hide();
    $("#Theme_transp").hide();


    if (AreaofIntid == "" || SelectedAreaofInt == "") {
        $.alert('Please Select Area of Interest');
    }
    else {
        $("#buffer").show();
    }
});


$("#CmplxBuff").click(function () {
    if (heatmapLayer != undefined) {
        map.removeLayer(heatmapLayer);
    }

    if (clusterLayer != undefined) {
