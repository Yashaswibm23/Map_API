$('.andor').click(function () {
    SelectedAnd_Or = $(this).attr("id");
    if (SelectedAnd_Or == 'and') {
        $('#and').css('background', 'url("symbols/And1-Btn.png")');
        $('#or').css('background', 'url("symbols/Or-Btn.png")');
    }
    if (SelectedAnd_Or == 'or') {
        $('#or').css('background', 'url("symbols/Or1-Btn.png")');
        $('#and').css('background', 'url("symbols/And-Btn.png")');
    }
});

$('.andor1').click(function () {
    SelectedAnd_Or1 = $(this).attr("name");

    if (SelectedAnd_Or1 == 'and') {
        $('#and1').css('background', 'url("symbols/And1-Btn.png")');
        $('#or1').css('background', 'url("symbols/Or-Btn.png")');
    }
    if (SelectedAnd_Or1 == 'or') {
        $('#or1').css('background', 'url("symbols/Or1-Btn.png")');
        $('#and1').css('background', 'url("symbols/And-Btn.png")');
    }
});

