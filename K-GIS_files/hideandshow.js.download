//***********Theme selection*****
$("#Transparency").click(function () {
    $("#queries").hide();
    $("#buffer").hide();
    $("#Cmplxbuffer").hide();
    $("#Theme_transp").show();
    if (heatmapLayer != undefined) {
        map.removeLayer(heatmapLayer);
    }

    if (clusterLayer != undefined) {
        map.removeLayer(clusterLayer);
    }
    for (var i = 0; i < removableLayersArr.length; i++) {
        map.removeLayer(removableLayersArr[i]);
    }
});
